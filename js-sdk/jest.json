{
 
  "coverageThreshold": {
    "global": {
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "moduleFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
    "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
  },
  "testRegex": "/src/.*\\.test\\.(ts|tsx|js|jsx)$",
  "updateSnapshot": true,
  "moduleNameMapper": {
    "^.*[.](jpg|JPG|gif|GIF|png|PNG|less|LESS|css|CSS|scss|SCSS)$": "<rootDir>/test_setup/EmptyModule"
  }
}